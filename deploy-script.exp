#!/usr/bin/expect -f

spawn sftp -P 23 $env(USER)@fentker.com
expect {Are you sure you want to continue connecting (yes/no/[fingerprint])? } {
        exp_send "yes\r"
        exp_continue
    }
expect "password:"
send "$env(PASS)"
expect "sftp>"
send "cd web && rm -rf jet && mkdir jet && cd jet && put -r"
expect "sftp>"
send "exit\n"        
interact
